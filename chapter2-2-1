workbook=#

SELECT							--取得する
	(usr.l_name || usr.f_name) AS ユーザ,		--ユーザテーブルの氏列と名列の結合テキストを別名「ユーザ」として指定
	COUNT(rent.*)					--貸出記録テーブルの集計列を指定
FROM				--どこから
	rental AS rent		--貸出記録テーブル(rental)
RIGHT JOIN			--内部結合
	usr		--ユーザテーブル(usr)か
ON					--結合キー
	rent.user_id = usr.user_id	--ユーザIDを結合キーに
GROUP BY			--グルーピング
	usr.l_name,		--氏でグルーピング
	usr.f_name		--名でグルーピング
ORDER BY			--ソート
	COUNT(rent.*) DESC	--貸し出し数が多い順に
;

